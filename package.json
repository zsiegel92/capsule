{
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev",
    "build": "prisma generate && prisma db push && next build",
    "start": "next start",
    "lint": "next lint",
    "email": "email dev --port=3001",
    "dev-python": "venv/bin/python3 -m uvicorn api.app:app --reload",
    "watch-gen-client": "npm-watch gen-client",
    "gen-client": "npx openapi-typescript-codegen -i api/openapi.json -o py_client --name PythonClient --useOptions --exportSchemas true",
    "fastRPC" : "concurrently \"npm run watch-gen-client\" \"npm run dev-python\""
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@prisma/extension-accelerate": "^0.6.2",
    "@react-email/button": "0.0.11",
    "@react-email/container": "^0.0.10",
    "@react-email/head": "^0.0.6",
    "@react-email/html": "^0.0.6",
    "@react-email/preview": "^0.0.7",
    "@react-email/render": "0.0.9",
    "@react-email/section": "^0.0.10",
    "@react-email/text": "^0.0.6",
    "@types/node": "^18.11.9",
    "@vercel/analytics": "^1.1.1",
    "bcrypt": "^5.1.0",
    "bootstrap": "^5.3.2",
    "concurrently": "^8.0.1",
    "next": "^14.0.2",
    "next-auth": "^4.22.1",
    "nodemailer": "^6.9.7",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.2",
    "react-dom": "^18.2.0",
    "react-email": "^1.9.5",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.11.0",
    "react-infinite-scroller": "^1.2.6",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-infinite-scroller": "^1.2.5",
    "autoprefixer": "^10.4.4",
    "eslint": "8.11.0",
    "eslint-config-next": "^13.0.5",
    "npm-watch": "^0.11.0",
    "openapi-typescript-codegen": "^0.25.0",
    "postcss": "^8.4.12",
    "prisma": "^5.5.2",
    "typescript": "^5.2.2"
  },
  "watch": {
    "gen-client": {
      "patterns": [
        "api"
      ],
      "extensions": "json"
    }
  }
}
